    <!--
      Node: <realitykit_refract>
      Note:
      From OSL codebase
        vector refract (vector I, vector N, float eta)
        {
            float IdotN = dot (I, N);
            float k = 1 - eta*eta * (1 - IdotN*IdotN);
            return (k < 0) ? vector(0,0,0) : (eta*I - N * (eta*IdotN + sqrt(k)));
        }
    -->
