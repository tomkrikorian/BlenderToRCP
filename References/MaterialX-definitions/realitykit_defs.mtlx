    <!--
      Node: <realitykit_unlit_surfaceshader>
      Notes :
        RealityKit implementation has additional checkbox that enable/disables the opacityThreshold functionality
        This isn't a real MaterialX parameter, instead the opacity thesholding is only enabled if there is any
        value connected to the opacityThreshold parameter (even if that value is the default of 0.0).
        We cannot construct a nodegraph that has the ability to determine if an input has a local value set (or
        connected to), so we're not able to re-create this behaviour.

      Unsupported Parameters :
        applyPostProcessToneMap
        hasPremultipliedAlpha
    -->
    <nodedef name="ND_realitykit_unlit_surfaceshader" node="realitykit_unlit_surfaceshader" nodegroup="realitykit" target="realitykit">
        <input name="color" type="color3" value="1, 1, 1" />
        <input name="opacity" type="float" value="1" />
        <input name="opacityThreshold" type="float" value="0" />
        <input name="applyPostProcessToneMap" type="boolean" value="true" uniform="true" />
        <input name="hasPremultipliedAlpha" type="boolean" value="false" uniform="true" />
        <output name="out" type="surfaceshader" />
    </nodedef>

