    <!--
      Node: <RealityKitTexture2D>, <RealityKitTexture2DLOD>, <RealityKitTexture2DGradient>,
        <RealityKitTexture2DPixel>, <RealityKitTexture2DPixelLOD>, <RealityKitTexture2DPixelGradient>

      Notes: Implemented as a wrapper around the standard MaterialX image node - which doesn't have as much
        control over image reading operation.

        Also note that RealityKit expects an inverted v texture coordinate relative to the MaterialX image node.

        Lots of assumptions/approximations taken here as the parameters don't align at all.
        * "u_wrap_mode" - ignored - hardcoded to 'clamp'
        * "v_wrap_mode" - ignored - hardcoded to 'clamp'
        * "border_color" - ignored
        * "mag_filter" - ignored - all filtering is 'linear'
        * "min_filter" - ignored - all filtering is 'linear'
        * "mip_filter" - ignored - all filtering is 'linear'
        * "max_anisotropy" - ignored
        * "max_lod_clamp" - ignored
        * "min_lod_clamp" - ignored
        * "bias" - ignored
        * "dynamic_min_lod_clamp" - ignored
        * "offset" - ignored.
            we intentionally remove the offset parameter because it has an unsupported type 'integer2' - this means
            the node definition diverges from the realityKit version
        * "lod" - ignored
        * "gradient_dPdx" - ignored
        * "gradient_dPdy" - ignored
    -->
    <nodedef name="ND_RealityKitTexture2D_color3" node="RealityKitTexture2D" nodegroup="texture2d" target="realitykit">
        <input name="file" type="filename"/>
        <input name="u_wrap_mode" type="string" uniform="true" enum="repeat,mirrored_repeat,clamp_to_edge,clamp_to_zero,clamp_to_border" value="clamp_to_edge" />
        <input name="v_wrap_mode" type="string" uniform="true" enum="repeat,mirrored_repeat,clamp_to_edge,clamp_to_zero,clamp_to_border" value="clamp_to_edge" />
        <input name="border_color" type="string" uniform="true" enum="transparent_black,opaque_black,opaque_white" value="transparent_black" />
        <input name="mag_filter" type="string" uniform="true" enum="nearest,linear" value="linear" />
        <input name="min_filter" type="string" uniform="true" enum="nearest,linear" value="linear" />
        <input name="mip_filter" type="string" uniform="true" enum="none,nearest,linear" value="linear" />
        <input name="max_anisotropy" type="integer" uniform="true" value="1" />
        <input name="max_lod_clamp" type="float" uniform="true" value="65504.0" />
        <input name="min_lod_clamp" type="float" uniform="true" value="0.0" />
        <input name="default" type="color3" value="0.0,0.0,0.0" />
        <input name="texcoord" type="vector2" defaultgeomprop="UV0" />
        <input name="no_flip_v" type="boolean" uniform="true" value="false" />
        <input name="bias" type="float" value="0.0" />
        <input name="dynamic_min_lod_clamp" type="float" value="0.0" />
        <input name="offset" type="integer2" value="0,0" />
        <output name="out" type="color3" />
    </nodedef>
